{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
  "noEmitOnError": true,
    "baseUrl": ".",
    "paths": {
      "@core/*": ["./core/*"],
      "@interface/*": ["./interface/*"]
    }
  },
  "include": ["core/**/*", "interface/**/*"],
  "exclude": ["node_modules"]
}
